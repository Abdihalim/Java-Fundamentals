System.out.println("The list of statement");
int t = 3;

if(t = 3)
{
System.out.println("The Value is 3");

if(t = 2)
{

Syste.out.println("The Value is 2");
 if (t = 0)
 {
 Syste.out.println("The number is 0");
}
else
